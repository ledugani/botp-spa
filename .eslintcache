 } = useAuth();\n\tconst history = useHistory();\n\n    async function handleLogout() {\n\t\tsetError('');\n\t\ttry {\n\t\t\tawait logout();\n\t\t\thistory.push('/login');\n\t\t} catch {\n\t\t\tsetError('Failed to log user out.')\n\t\t}\n\t}\n\n    useEffect(() => {\n\t\tuserRef.get().then((doc) => {\n\t\t\tif (doc.exists) {\n\n\t\t\t\tconst artistsFromFb = doc.data().artists;\n\n\t\t\t\tsetArtists(artistsFromFb);\n\n\t\t\t\tconst artistsStringified = artistsFromFb.toString().replaceAll(',','%');\n\n\t\t\t\taxios(`https://accounts.spotify.com/api/token`, {\n\t\t\t\t\theaders: {\n\t\t\t\t\t\t'Content-Type' : 'application/x-www-form-urlencoded',\n\t\t\t\t\t\t'Authorization' : 'Basic ' + btoa(spotify.ClientId + ':' + spotify.ClientSecret)\n\t\t\t\t\t},\n\t\t\t\t\tdata: 'grant_type=client_credentials',\n\t\t\t\t\tmethod: 'POST'\n\t\t\t\t}).then((response) => {\n\t\t\t\t\tconsole.log(response.data.access_token)\n\t\t\t\t\taxios(`https://api.spotify.com/v1/artists?ids=${artistsStringified}`, {\n\t\t\t\t\t\tmethod: 'GET',\n\t\t\t\t\t\theaders: {'Authorization' : 'Bearer ' + response.data.access_token}\n\t\t\t\t\t})\n\t\t\t\t\t.then((res) => {\n\t\t\t\t\t\tconsole.log(res);\n\t\t\t\t\t})\n\t\t\t\t})\n\t\t\t\t// doc.data() will be undefined in this case\n\t\t\t} else {\n\t\t\t\tconsole.log(\"No such document!\");\n\t\t\t}\n\t\t}).catch((error) => {\n\t\t\tconsole.log(\"Error getting document:\", error);\n\t\t});\n\t}, []);\n    \n    return (\n\t\t<>\n\t\t\t<Seo title='My Artists' />\n\t\t\t<Container className='page-container'>\n                <h1>Your Saved Artists</h1>\n                <ul>\n                    {\n                        artists.map((artist) => {\n                            return <li key={artist}>{artist}</li>;\n                        })                        \n                    }\n                </ul>\n                <div className='w-100 text-center mt-2'>\n                    <Button\n                        variant='link'\n                        onClick={handleLogout}\n                    >\n                        Log Out\n                    </Button>\n\n                    {error && <Alert variant='danger'>{error}</Alert>}\n                </div>\n\t\t\t</Container>\n\t\t</>\n\t);\n}","/Users/thomasdugan/workspace/botp-spa/src/app/auth/Auth.js",[],"/Users/thomasdugan/workspace/botp-spa/src/app/private.jsx",[],"/Users/thomasdugan/workspace/botp-spa/src/firebase.js",[],"/Users/thomasdugan/workspace/botp-spa/src/app/layout/index.jsx",[],"/Users/thomasdugan/workspace/botp-spa/src/app/auth/pwreset/index.jsx",[],"/Users/thomasdugan/workspace/botp-spa/src/app/auth/signup/index.jsx",[],"/Users/thomasdugan/workspace/botp-spa/src/app/auth/login/index.jsx",[],"/Users/thomasdugan/workspace/botp-spa/src/pages/dashboard/index.jsx",[],"/Users/thomasdugan/workspace/botp-spa/src/pages/homepage/index.jsx",[],{"ruleId":"100","severity":1,"message":"101","line":65,"column":5,"nodeType":"102","endLine":65,"endColumn":7,"suggestions":"103"},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'spotify.ClientId', 'spotify.ClientSecret', and 'userRef'. Either include them or remove the dependency array.","ArrayExpression",["104"],{"desc":"105","fix":"106"},"Update the dependencies array to be: [spotify.ClientId, spotify.ClientSecret, userRef]",{"range":"107","text":"108"},[1965,1967],"[spotify.ClientId, spotify.ClientSecret, userRef]"]